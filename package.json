{
  "name": "video-feed",
  "version": "0.1.0",
  "homepage": "https://dexmac.github.io/video-feed-redux/",
  "private": true,
  "proxy": "http://localhost:4000/",
  "dependencies": {
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "express": "^4.16.2",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": ">=16.3.3",
    "react-player": "^1.3.0",
    "react-redux": "^5.0.7",
    "react-scripts": "^1.1.1",
    "react-select": "^1.2.1",
    "react-test-renderer": "^16.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.4"
  },
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"yarn run server\" \"yarn run client\"",
    "client": "react-scripts start",
    "server": "node api/server.js",
    "test": "BABEL_ENV=test jest",
    "tdd": "BABEL_ENV=test jest --watch",
    "lint": "eslint --ignore-pattern 'src/styles/*' -- src/**",
    "lint:fix": "eslint --fix --ignore-pattern 'src/styles/*' -- src/**",
    "quality-check": "yarn run lint && yarn run test",
    "build": "yarn run quality-check && react-scripts build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "webpack-dev-server": ">=3.1.11",
    "react-dev-utils": ">=5.0.2",
    "merge": ">=1.2.1",
    "gh-pages": "^1.2.0",
    "redux-mock-store": "^1.5.1",
    "fetch-mock": "^6.4.3",
    "babel-cli": "^6.26.0",
    "concurrently": "^3.5.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "identity-obj-proxy": "^3.0.0",
    "prettier": "^1.11.1",
    "raf": "^3.4.0"
  },
  "pre-commit": [
    "build"
  ],
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/registerServiceWorker.js",
      "!src/helpers/fetchJSON.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/index.js",
      "src/services"
    ],
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50 
      }
    }
  },
  "author": "dexmac",
  "license": "Apache 2"
}
